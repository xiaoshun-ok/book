<view class="regBg"><view class="regBox"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="input-flex"><view class="input-flex-label">用户名</view><input class="input-flex-text" name="username" type="text" placeholder="请填用户名" data-event-opts="{{[['blur',[['checkUserName',['$event']]]]]}}" bindblur="__e"/></view><view class="input-flex"><view class="input-flex-label">昵称</view><input class="input-flex-text" name="nickname" type="text" placeholder="请填写昵称" data-event-opts="{{[['blur',[['checkNickName',['$event']]]]]}}" bindblur="__e"/></view><view class="input-flex"><view class="input-flex-label">邮箱</view><input class="input-flex-text" name="email" type="text" placeholder="找回密码工具" data-event-opts="{{[['blur',[['checkEmail',['$event']]]]]}}" bindblur="__e"/></view><view class="input-flex"><view class="input-flex-label">密码</view><input class="input-flex-text" name="password" type="text" password="true" placeholder="请填写密码" data-event-opts="{{[['blur',[['checkPassWord',['$event']]]],['confirm',[['checkPassWord',['$event']]]]]}}" bindblur="__e" bindconfirm="__e"/></view><button class="btn-row-submit btn-success" disabled="{{check}}" type="primary" form-type="submit">立即注册</button></form></view><uni-popup class="vue-ref" vue-id="1" show="{{showPopup}}" type="bottom" data-ref="popup" bind:__l="__l" vue-slots="{{['default']}}">{{popupMessage}}</uni-popup></view>